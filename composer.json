{
  "name": "nitsan/ns-twitter",
  "type": "typo3-cms-extension",
  "description": "Twitter extension brings all your recent tweets to your TYPO3 site",
  "homepage": "https://extensions.typo3.org/extension/ns_twitter/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "twitter",
    "Nitsan"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_twitter/issues"
  },
  "require": {
    "typo3/cms-core": "^7.6.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsTwitter\\": "Classes/"
    }
  },
  "replace": {
      "ns_twitter": "self.version",
      "nitsan/ns-twitter": "self.version"
  },
  "extra": {
      "typo3/cms": {
          "extension-key": "ns_twitter"
      }
  }
}